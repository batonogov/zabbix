SHELL=/bin/bash
BASH_ENV=/container.env

@hourly bash /etc/cron.hourly/* >> /var/log/cron/hourly.log 2>&1
@daily bash /etc/cron.daily/* >> /var/log/cron/daily.log 2>&1
@weekly bash /etc/cron.weekly/* >> /var/log/cron/weekly.log 2>&1
@monthly bash /etc/cron.monthly/* >> /var/log/cron/monthly.log 2>&1
